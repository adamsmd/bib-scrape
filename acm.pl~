use LWP::Simple;

my $url = $ARGS[1];
print "url: <$url>\n";
$page = get($url);
m[<meta name="citation_doi" content="10.1145/78973.78977"]m;

my ($par, $id) = $page =~ m[<meta name="citation_abstract_html_url" content="http://portal.acm.org/citation.cfm?id=(\d+)\.(\d+)">]m;

